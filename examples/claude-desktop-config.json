{
  "mcpServers": {
    "Hala.DatabaseMcpAgent": {
      "command": "dnx",
      "args": [
        "Hala.DatabaseMcpAgent@1.0.10-preview",
        "--yes"
      ],
      "env": {
        "OracleConnectionString": "YOUR_ORACLE_CONNECTION_STRING_HERE",
        "MetadataCacheJsonPath": "YOUR_METADATA_JSON_PATH_HERE", 
        "LogFilePath": "YOUR_LOG_FILE_PATH_HERE"
      }
    }
  }
}